<% var flash = locals['flash'] %> 
<% if (flash && Object.keys(flash).length !== 0) { var type = Object.keys(flash)[0]; var message = flash[type] %>
    <div class="text-center alert alert-<%- type %> alert-dismissible" style="position: fixed; top: 90px; z-index: 9999999; width: 50%; left: 25%;">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
         <%- message %>
    </div>
<% } %> 

